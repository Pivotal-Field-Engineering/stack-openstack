{
    "name": "os-ha-messaging",
    "description": "Highly available messaging server role",
    "json_class": "Chef::Role",
    "default_attributes": { },
    "override_attributes": { 
        "rabbitmq": {
            "version": "3.3.5",
            "cluster": true,
            "ssl": true,
            "web_console_ssl": true,
            "disabled_policies": [ "ha-two" ]
        }
    },
    "chef_type": "role",
	"run_list" : [ 
		"recipe[sysutils::default]",
        "recipe[network::default]",
        "recipe[openstack-services::ha-messaging]"
	]
}
